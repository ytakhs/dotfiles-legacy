{
    "breadcrumbs.enabled": true,
    "editor.fontSize": 10,
    "editor.tabSize": 2,
    "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.cursorStyle": "line-thin",
    "editor.suggestSelection": "first",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "'Meslo LG M DZ for Powerline', 'Delugia Nerd Font'",
    "terminal.integrated.shell.linux": "/usr/local/bin/fish",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "One Monokai",
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.enablePreview": false,
    "ruby.codeCompletion": "rcodetools",
    "ruby.format": "standard",
    "ruby.intellisense": "rubyLocate",
    "ruby.lint": {
        "ruby": true,
    },
    "ruby.locate": {
        "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}",
        "include": "**/*.rb"
    },
    "ruby.useLanguageServer": true,
    "search.useIgnoreFiles": false,
    "solargraph.diagnostics": true,
    "solargraph.completion": true,
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-e>": false,
        "<C-a>": false,
        "<C-n>": false,
    },
    "vscode-advanced-open-file.selectPath": false,
    "vscode-advanced-open-file.groupDirectoriesFirst": true,
    "[go]": {
        "editor.tabSize": 4
    },
    "[rust]": {
        "editor.tabSize": 4,
        "editor.formatOnSave": true
    },
    "[c]": {
        "editor.tabSize": 4
    },
    "[python]": {
        "editor.tabSize": 4
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[makefile]": {
        "editor.tabSize": 4
    }
}
