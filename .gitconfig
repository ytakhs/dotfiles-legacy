[user]
  name  = jtakh
  email = 21127627+jit-y@users.noreply.github.com
[core]
  excludesfile = ~/.gitignore_global
  pager = delta --paging never
[ghq]
  root = ~/dev/.ghq
[credential]
[pager]
  log   = delta --paging never | less -+$LESS -R
  show  = delta --paging never | less -+$LESS -R
  diff  = delta --paging never | less -+$LESS -R
  blame = delta --paging never | less -+$LESS -R
[alias]
  st = status
  br = branch
  co = checkout
  cm = commit -m
[color]
  branch = auto
  diff = auto
  status = auto
  interactive = auto
  ui = true
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
